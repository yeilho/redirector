{
  "manifest_version": 3,
  "name": "URL Redirector",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "omnibox": {
    "keyword": "go"
  }
}
